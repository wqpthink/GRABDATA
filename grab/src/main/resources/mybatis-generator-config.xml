<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >

<generatorConfiguration>
    <!--本地数据库驱动程序jar包的全路径-->
    <classPathEntry location="D:\\software\\maven\\repository\\mysql\\mysql-connector-java\\6.0.6\\mysql-connector-java-6.0.6.jar"/>

    <context id="context" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="javaFileEncoding" value="UTF-8" />
            <property name="suppressAllComments" value="false"/>
            <property name="suppressDate" value="false"/>
        </commentGenerator>

        <!--数据库的相关配置-->
        <jdbcConnection
            driverClass="com.mysql.jdbc.Driver"
            connectionURL="jdbc:mysql://localhost:3306/grabdata?serverTimezone=UTC"
            userId="root"
            password="123456"/>

        <!--非必需，类型处理器，在数据库类型和java类型之间的转换控制-->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!--实体类生成的位置-->
        <javaModelGenerator targetPackage="com.wqp.grab.module.emr.entity" targetProject="src/main/java">
            <property name="trimStrings" value="true" />
            <property name="enableSubPackages" value="false"/>
            <property name="constructorBased" value="false"/>
        </javaModelGenerator>

        <!--Mapper映射文件生成所在的目录 为每一个数据库的表生成对应的*Mapper.xml文件-->
        <sqlMapGenerator targetPackage="com.wqp.grab.module.emr.mapper" targetProject="src/main/java"/>

        <!--Mapper 接口文件的位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.wqp.grab.module.emr.dao" targetProject="src/main/java"/>

        <!--相关表的配置-->
        <!--<table tableName="win_history" domainObjectName="WinHistory"-->
            <!--enableInsert="true"-->
            <!--enableUpdateByExample="false"-->
            <!--enableCountByExample="false"-->
            <!--enableDeleteByExample="false"-->
            <!--enableSelectByExample="false"-->
            <!--selectByExampleQueryId="false"></table>-->
        <!--<table tableName="recommend" domainObjectName="Recommend"-->
           <!--enableInsert="true"-->
           <!--enableUpdateByExample="false"-->
           <!--enableCountByExample="false"-->
           <!--enableDeleteByExample="false"-->
           <!--enableSelectByExample="false"-->
           <!--selectByExampleQueryId="false"></table>-->
        <table tableName="his_emr_adjunct" domainObjectName="HisEmrAdjunct"
               enableInsert="true"
               enableUpdateByExample="false"
               enableCountByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"></table>

    </context>

</generatorConfiguration>